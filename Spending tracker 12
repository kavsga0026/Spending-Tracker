package db;
import java.sql.*;
/**
 *
 * @author vsga0
 */
public class DbConnect {
    public static Connection c;
    public static Statement s;
    static{
        try{
            c=DriverManager.getConnection("jdbc:mysql://localhost:3306/spendingdb"+"?useSSL=false","root","Incapp@12");
            s=c.createStatement();
        }catch(Exception ex){
            JOptionPane.showMessageDialog(null,ex);
        }
    }
    
}
